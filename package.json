{
  "name": "nextjs-theme-tailwindcss",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "cypress": "cypress open",
    "check:types": "tsc --pretty --noEmit",
    "coverage": "jest --coverage --watchAll=true",
    "lint:fix": "next lint --fix --resolve-plugins-relative-to \"./**/*.{ts,tsx}\"",
    "lint:report": "next lint src/ -f ../../../node_modules/eslint-html-reporter/reporter.js -o ../../../docs/lint/report.html",
    "make-badges": "npm run coverage && istanbul-badges-readme --logo='jest'",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "test:quick": "jest --watchAll=false",
    "test:detail": "TZ=America/Los_Angeles jest --watchAll --verbose",
    "test:coverage": "jest --coverage --watchAll=false",
    "prettier": "prettier --write ./**/*.{ts,tsx,css}",
    "prettify": "npm run check:types && npm run prettier && npm run lint:fix",
    "precommit": "npm run prettify && npm run build && npm run test:quick"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.14",
    "@heroicons/react": "^2.0.17",
    "@tailwindcss/forms": "^0.5.3",
    "@tanstack/react-query": "^4.29.3",
    "@types/node": "18.15.11",
    "@types/react": "18.0.35",
    "@types/react-dom": "18.0.11",
    "autoprefixer": "10.4.14",
    "clsx": "^1.2.1",
    "cookies-next": "^2.1.1",
    "eslint": "^8.38.0",
    "eslint-config-next": "13.3.0",
    "next": "13.3.0",
    "postcss": "8.4.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.1",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/cypress": "^1.1.3",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "cypress": "^12.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1"
  }
}
